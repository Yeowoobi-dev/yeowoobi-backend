{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/user/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/user/tsconfig.app.json",
    "plugins": [
      "@nestjs/swagger"
    ]
  },
  "monorepo": true,
  "root": "apps/user",
  "projects": {
    "user": {
      "type": "application",
      "root": "apps/user",
      "entryFile": "main",
      "sourceRoot": "apps/user/src",
      "compilerOptions": {
        "tsConfigPath": "apps/user/tsconfig.app.json"
      }
    },
    "book-log": {
      "type": "application",
      "root": "apps/book-log",
      "entryFile": "main",
      "sourceRoot": "apps/book-log/src",
      "compilerOptions": {
        "tsConfigPath": "apps/book-log/tsconfig.app.json"
      }
    }
  }
}